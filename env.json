// Route managing key for 'aws-lambda-ws-server'
// Default is 'message', while 'action' is AWS default
{
    "SendMessageFunction": {
        "AWS_REGION_NAME": "local",
        "DYNAMO_END_POINT": "http://host.docker.internal:8000",
        "TABLE_NAME": "simplechat_connections",
        "MAPPING_KEY": "action"
    },
    "OnConnectFunction": {
        "AWS_REGION_NAME": "local",
        "DYNAMO_END_POINT": "http://host.docker.internal:8000",
        "TABLE_NAME": "simplechat_connections",
        "MAPPING_KEY": "action"
    },
    "OnDisconnectFunction": {
        "AWS_REGION_NAME": "local",
        "DYNAMO_END_POINT": "http://host.docker.internal:8000",
        "TABLE_NAME": "simplechat_connections",
        "MAPPING_KEY": "action"
    }
}